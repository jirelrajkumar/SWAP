<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.codepath.rkpandey.SocialWatchParty"
    >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cinema"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cinema"
        android:supportsRtl="true"
        android:theme="@style/Theme.SWaP"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.codepath.rkpandey.SocialWatchParty.Play_act"
            android:exported="true" />
        <activity
            android:name="com.codepath.rkpandey.SocialWatchParty.YouTubeMainActivity"
            android:exported="true" />
        <activity
            android:name="com.codepath.rkpandey.SocialWatchParty.HomeActivity"
            android:exported="true" />
        <activity
            android:name="com.codepath.rkpandey.SocialWatchParty.splashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.codepath.rkpandey.SocialWatchParty.MainActivity"
            android:exported="true" />
    </application>

</manifest>